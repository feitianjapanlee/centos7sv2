<!-- download.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Download</title>
</head>
<body>
    <h1>Download Files</h1>
    <ul>
        <% files.forEach(file => { %>
            <li><a href="/myapp/download/<%= file %>"><%= file %></a></li>
        <% }) %>
    </ul>
</body>
</html>